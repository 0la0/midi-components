    "lint": "eslint src"
  },
  "devDependencies": {
    "eslint": "^5.9.0",
    "esm": "^3.0.84",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^5.2.0",
    "raw-loader": "^0.5.1",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "dependencies": {
    "three": "^0.92.0"
  }
}
