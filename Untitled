navigator.mediaDevices.getUserMedia({ audio: true })
.then(stream =>
    navigator.mediaDevices.enumerateDevices()
      .then(devices => {
        var TR_09 = 'TR-09';
        var input = devices.find(device => device.kind === 'audioinput' && device.label === TR_09)
        var output = devices.find(device => device.kind === 'audiooutput' && device.label === TR_09)
        return { input, output };
      })
)
.then(tr09 => {
  console.log('tr09', tr09)
  navigator.mediaDevices.getUserMedia({ audio: { deviceId: { exact: tr09.output.deviceId } } })
    .then(tr09stream => {
      console.log('tr09 stream?', tr09stream)
      var audioContext = new AudioContext();
      var mediaStreamSource = audioContext.createMediaStreamSource( tr09stream );
      mediaStreamSource.connect( audioContext.destination );
    })
})
.catch(err => console.error(err))
